        "import re\n",
        "\n",
        "def square(match) :\n",
        "  \"Return square value\"\n",
        "  value = int(match.group())\n",
        "  new = value * value\n",
        "  return str(new)\n",
        "\n",
        "p = re.compile(r\"\\d+\")\n",
        "p.sub(square, \"Please, square the following numbers, 3 7 11 13 17 19\")"
      ],
